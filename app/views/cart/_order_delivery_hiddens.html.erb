<%- fields_for :order_delivery do | od | -%>
  <% @order_delivery.attribute_names.each do | column | %>
    <% unless @order_delivery[column].blank? or column == "use_point" or column == "payment_id" or column =="delivery_time_id"%>
	  <%= od.hidden_field column %>
	<% end %>
  <% end %>
<%- end -%>

<% if @not_login_customer%>
<%=hidden_field_tag "temporary_customer_flag","1"%>
  <% if !params[:address_enable].nil?%>
    <%=hidden_field_tag "address_enable","1"%>
  <% end%>  
<% end%>
